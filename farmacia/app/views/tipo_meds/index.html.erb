<p id="notice"><%= notice %></p>

<h1>Listing Tipo Meds</h1>

<div class="col-lg-12">
                <div class="block">
                  <div class="title"><strong> Tipo de Medicamento</strong></div>
                  <div class="table-responsive"> 
                    <table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Tipo descrip</th>
      <th>Tipo active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_meds.each do |tipo_med| %>
      <tr>
        <td><%= tipo_med.tipo_descrip %></td>
        <td><%= tipo_med.tipo_active %></td>
        <td><%= link_to 'Show', tipo_med %></td>
        <td><%= link_to 'Edit', edit_tipo_med_path(tipo_med) %></td>
        <td><%= link_to 'Destroy', tipo_med, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
                </div>
              </div>
              </div>
          </div>
 </section>
<br>

<%= link_to 'New Tipo med', new_tipo_med_path %>
