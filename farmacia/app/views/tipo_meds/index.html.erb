<p id="notice"><%= notice %></p>

<h1>Listing Tipo Meds</h1>

<table class="table table-warning">
  <thead>
    <tr>
      <th>Tipo descrip</th>
      <th>Tipo active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_meds.each do |tipo_med| %>
      <tr>
        <td><%= tipo_med.tipo_descrip %></td>
        <td><%= tipo_med.tipo_active %></td>
        <td><%= link_to 'Show', tipo_med %></td>
        <td><%= link_to 'Edit', edit_tipo_med_path(tipo_med) %></td>
        <td><%= link_to 'Destroy', tipo_med, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo med', new_tipo_med_path %>
